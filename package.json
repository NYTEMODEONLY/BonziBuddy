{
  "name": "bonzibuddy",
  "private": true,
  "description": "Your very own desktop personal digital assistant",
  "version": "5.0.0",
  "author": "Leondro Lio <github@leondrolio.com>",
  "main": "electron.js",
  "dependencies": {
    "clippyjs": "^0.0.3",
    "electron-serve": "^2.0.0",
    "electron-store": "^8.1.0",
    "gatsby": "^5.13.0",
    "gatsby-plugin-manifest": "^5.13.0",
    "gatsby-plugin-react-helmet": "^6.13.0",
    "gatsby-plugin-sass": "^6.13.0",
    "gatsby-plugin-sharp": "^5.13.0",
    "gatsby-source-filesystem": "^5.13.0",
    "gatsby-transformer-sharp": "^5.13.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.77.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^33.0.0",
    "electron-builder": "^25.0.0",
    "prettier": "^3.2.0",
    "wait-on": "^7.2.0"
  },
  "keywords": [
    "bonzi"
  ],
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "start:dev": "concurrently \"gatsby develop\" \"wait-on http://localhost:8000 && cross-env NODE_ENV=development electron .\"",
    "start": "cross-env NODE_ENV=development electron .",
    "bonzi": "cross-env NODE_ENV=production electron .",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "generate-icons": "./scripts/generate-icons.sh",
    "package:mac": "gatsby build && electron-builder --mac",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/7coil/BonziBuddy"
  },
  "bugs": {
    "url": "https://github.com/7coil/BonziBuddy/issues"
  }
}
